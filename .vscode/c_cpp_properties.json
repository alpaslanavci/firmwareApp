{
    "configurations": [
        {
            "name": "STM32L073RZ",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/libopencm3/include",
                "${workspaceFolder}/app/inc",
                "${workspaceFolder}/bootloader/inc"
            ],
            "defines": [
                "STM32L0",
                "STM32L073RZ"
